{
  "name": "covid-graph-graphql",
  "version": "1.0.0",
  "description": "A GraphQL API for the CovidGraph project",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "cross-env DEBUG=neo4j-graphql-js ./node_modules/.bin/nodemon --watch src --ext js,graphql --exec babel-node  src/index.js",
    "build": "babel src --out-dir build && shx cp .env build 2>/dev/null || : && shx cp src/schema.graphql build",
    "now-build": "babel src --out-dir build && shx cp src/schema.graphql build",
    "start": "npm run build && node build/index.js",
    "integration": "nyc --reporter=lcov ava --timeout=15s ./src/schemas/**/tests/*.test.js --verbose"
  },
  "author": "Michael Graham",
  "license": "Apache-2.0",
  "dependencies": {
    "apollo-server": "^2.14.2",
    "apollo-server-lambda": "^2.14.2",
    "csv-parse": "^4.10.1",
    "dotenv": "^7.0.0",
    "graphql-tools": "^6.2.4",
    "neo4j-driver": "^4.0.2",
    "neo4j-graphql-js": "^2.16.4",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.5",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.2",
    "ava": "^3.13.0",
    "babel-plugin-auto-import": "^1.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "graphql-tag": "^2.11.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.19.1",
    "nyc": "^15.1.0",
    "shx": "^0.3.2"
  }
}